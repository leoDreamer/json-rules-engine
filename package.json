{
  "name": "jsonre",
  "version": "0.0.8",
  "description": "Rules Engine expressed in simple json",
  "main": "index.js",
  "scripts": {
    "test": "mocha && npm run lint --silent",
    "lint": "standard --verbose --env mocha | snazzy || true",
    "lint:fix": "standard --fix --env mocha",
    "build": "babel --stage 1 -d dist/ src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leoDreamer/json-rules-engine"
  },
  "keywords": [
    "rules",
    "engine",
    "rules engine"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist"
    ],
    "globals": [
      "context",
      "xcontext",
      "describe",
      "xdescribe",
      "it",
      "xit",
      "before",
      "beforeEach",
      "expect",
      "factories"
    ]
  },
  "files": [
    "src",
    "examplesb",
    "docs",
    "test",
    "index.js"
  ],
  "author": "Leo <leolichuan@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leoDreamer/json-rules-engine/issues"
  },
  "homepage": "https://github.com/leoDreamer/json-rules-engine",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "colors": "~1.3.3",
    "dirty-chai": "2.0.1",
    "mocha": "^6.0.2",
    "perfy": "^1.1.5",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "clone": "^2.1.2",
    "events": "^3.0.0",
    "lodash.isobjectlike": "^4.0.0",
    "object-hash": "^1.3.1",
    "selectn": "^1.1.2"
  }
